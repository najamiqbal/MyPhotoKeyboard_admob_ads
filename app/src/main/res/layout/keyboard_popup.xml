<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="@drawable/keyboard_popup_panel_background"
        android:paddingLeft="16dip"
        android:paddingRight="16dip"
        >
    <keyboard.cheetahphotokeyboard.neon.saad_Keybord_BaseImg_View
            xmlns:latin="http://schemas.android.com/apk/res/keyboard.cheetahphotokeyboard.neon"
            android:id="@+id/basiimgview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@android:color/white"
            latin:keyBackground="@drawable/btn_keyboard_key_gingerbread_popup"
            latin:keyHysteresisDistance="0dip"
            latin:verticalCorrection="@dimen/mini_keyboard_vertical_correction"
            latin:keyPreviewLayout="@layout/empty_activity"
            latin:popupLayout="@layout/empty_activity"
            />
</LinearLayout>
